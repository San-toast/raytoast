<!DOCTYPE html>
<html lang="en">
  <%- include('../templates/header')-%>
  <body>
    <%- include('../templates/nav')-%>
    <div class="contact-header">
      <p class="contact-title">Contact</p>
      <p class="contact-text">Enter your information to reserve your ticket</p>
    </div>
    <div class="page-container">
      <div class="contact-container">
        <form action="redirect">
          <p class="form-title">First Name</p>
          <input type="text" class="first-name" />
          <p class="form-title">Last Name</p>
          <input type="text" class="last-name" />
          <p class="form-title">Email</p>
          <input type="text" class="email" />
          <button class="submitButton grow">Submit</button>
        </form>
      </div>
    </div>
  </body>

  <script src="../js/createTicket.js"></script>
</html>
